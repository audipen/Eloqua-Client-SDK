<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <solution>
    <add key="disableSourceControlIntegration" value="true" />

    <!-- Enable package restore from local and official package sources -->
    <packageSources>
      <add key="NuGet official package source" value="https://nuget.org/api/v2/" />
      <add key="LocalPackageSource" value="..\NuGetRepository" />
    </packageSources>
    <disabledPackageSources />
    <activePackageSource>
      <add key="All" value="(Aggregate source)"  />
    </activePackageSource>
  </solution>
</configuration>